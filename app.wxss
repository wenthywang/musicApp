/**app.wxss**/

page{
    background-color: #f5f5f9;
}
.container {
  min-height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #f5f5f9;
  font-size: 14px;
} 

@font-face {
  font-family: 'iconfont';  /* project id 545342 */
  src: url('//at.alicdn.com/t/font_545342_rlryge1mzi7zm2t9.eot');
  src: url('//at.alicdn.com/t/font_545342_rlryge1mzi7zm2t9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_545342_rlryge1mzi7zm2t9.woff') format('woff'),
  url('//at.alicdn.com/t/font_545342_rlryge1mzi7zm2t9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_545342_rlryge1mzi7zm2t9.svg#iconfont') format('svg');
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:14px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}

.icon-right:before { content: "\e88e"; }/*右箭头*/
.icon-success:before { content: "\e654"; }/*成功*/
.icon-close:before { content: "\e7be"; }/*关闭*/

.icon-email:before { content: "\e6d3"; }/*邮箱*/
.icon-about:before { content: "\e6cd"; }/*关于我们*/
.icon-gift:before { content: "\e655"; }/*礼物*/
.icon-notice:before { content: "\e643"; }/*喇叭*/

